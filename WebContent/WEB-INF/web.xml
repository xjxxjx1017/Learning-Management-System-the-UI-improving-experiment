<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="false" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Image</servlet-name>
    <servlet-class>ImageServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Image</servlet-name>
    <url-pattern>/image/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AccountServlet</servlet-name>
    <servlet-class>com.xjxxjx1017.AccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountServlet</servlet-name>
    <url-pattern>/accountservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WebContentServlet</servlet-name>
    <servlet-class>com.xjxxjx1017.WebContentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WebContentServlet</servlet-name>
    <url-pattern>/webcontentservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileServlet</servlet-name>
    <servlet-class>com.xjxxjx1017.FileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileServlet</servlet-name>
    <url-pattern>/fileservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileServletBinded</servlet-name>
    <servlet-class>com.xjxxjx1017.FileServletBinded</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileServletBinded</servlet-name>
    <url-pattern>/fileservletbinded</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileDownloadServlet</servlet-name>
    <servlet-class>com.xjxxjx1017.FileDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileDownloadServlet</servlet-name>
    <url-pattern>/downloadmega</url-pattern>
  </servlet-mapping>
</web-app>